<environment title='Fig4'>
    <!-- simulation parameters -->
    <parameter type='num_sweeps' value='50'></parameter>
    <parameter type='num_simulations' value='1000'></parameter>
    <parameter type='num_agents' value='15'></parameter>

    <!-- state parameters -->
    <parameter type='theta' value='1'></parameter>
    <parameter type='rho' value='0.0'></parameter>
    <parameter type='lamb' value='0.0'></parameter>

    <!-- utility function parameters -->
    <parameter type='alpha' value='0.01'></parameter>
    <parameter type='beta' value='0.0'></parameter>
    <parameter type='gamma' value='0.1'></parameter>

    <!-- signal parameters -->
    <parameter type='signal0_mean' value='0.49'></parameter>
    <parameter type='signal0_variance' value='0.1'></parameter>
    <parameter type='signal1_mean' value='0.51'></parameter>
    <parameter type='signal1_variance' value='0.1'></parameter>

    <!-- network structure parameters -->
    <parameter type='fixed_network' value='True'></parameter>
    <parameter type='network_density' value='0.5'></parameter>
    <parameter type='network_topology' value='random'></parameter>

    <!-- -->
    <!-- parameters required to get figures for results -->
    <!-- -->
    <parameter type='agent_heterogeneity' value='True'></parameter>
    <parameter type='agent_heterogeneity_parameter' value='0.4'></parameter>

    <!-- -->
    <!-- parameters required to compute goodness -->
    <!-- -->
    <parameter type='compute_goodness' value='True'></parameter>
    <parameter type='theta_min' value='0'></parameter>
    <parameter type='theta_max' value='1'></parameter>
    <parameter type='theta_step' value='1'></parameter>

    <parameter type='signal_dist_min' value='0.02'></parameter>
    <parameter type='signal_dist_max' value='0.9'></parameter>
    <parameter type='signal_dist_step' value='0.1'></parameter>

    <parameter type='signal_var_min' value='0.02'></parameter>
    <parameter type='signal_var_max' value='0.25'></parameter>
    <parameter type='signal_var_step' value='0.05'></parameter>
</environment>